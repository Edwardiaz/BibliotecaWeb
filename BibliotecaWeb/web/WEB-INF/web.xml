<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Usuario</servlet-name>
        <servlet-class>Controlador.Usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioControlador</servlet-name>
        <servlet-class>Controlador.UsuarioControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RolControlador</servlet-name>
        <servlet-class>Controlador.RolControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutorControlador</servlet-name>
        <servlet-class>Controlador.AutorControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArtistaControlador</servlet-name>
        <servlet-class>Controlador.ArtistaControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DirectorControlador</servlet-name>
        <servlet-class>Controlador.DirectorControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditorialControlador</servlet-name>
        <servlet-class>Controlador.EditorialControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeneroControlador</servlet-name>
        <servlet-class>Controlador.GeneroControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginControlador</servlet-name>
        <servlet-class>Controlador.LoginControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Usuario</servlet-name>
        <url-pattern>/Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioControlador</servlet-name>
        <url-pattern>/UsuarioControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RolControlador</servlet-name>
        <url-pattern>/RolControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutorControlador</servlet-name>
        <url-pattern>/AutorControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ArtistaControlador</servlet-name>
        <url-pattern>/ArtistaControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DirectorControlador</servlet-name>
        <url-pattern>/DirectorControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditorialControlador</servlet-name>
        <url-pattern>/EditorialControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneroControlador</servlet-name>
        <url-pattern>/GeneroControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginControlador</servlet-name>
        <url-pattern>/LoginControlador</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
